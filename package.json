{
  "publisher": "simonhe",
  "name": "simon-plugin",
  "displayName": "simon-plugin",
  "version": "0.0.1",
  "description": "A comprehensive VSCode plugin collection by Simon He.",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/simon-plugin#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/simon-plugin"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/simon-plugin/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "keywords": [
    "vscode",
    "vscode-use",
    "vscode-template",
    "Simon He",
    "VSCode plugins",
    "developer tools",
    "productivity"
  ],
  "categories": [
    "Extension Packs"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.95.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "dev": "nr build",
    "test": "vitest",
    "build": "tsup",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "nr lint --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "nr build --minify && bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.1",
    "@types/node": "^18.19.64",
    "@types/vscode": "^1.95.0",
    "@vscode-use/utils": "0.1.32",
    "@vscode/vsce": "^3.2.1",
    "bumpp": "^9.8.1",
    "eslint": "^9.14.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5"
  },
  "extensionPack": [
    "simonhe.vscode-icones",
    "simonhe.vscode-transfer",
    "simonhe.vscode-open-pkg",
    "simonhe.to-ts-type",
    "simonhe.vscode-json-structure",
    "simonhe.vscode-tobase64",
    "simonhe.vscode-alias-jump",
    "simonhe.log",
    "simonhe.vscode-emoji",
    "simonhe.vscode-error",
    "simonhe.vscode-fast-empty",
    "simonhe.trim-paste",
    "simonhe.daily-planner",
    "simonhe.vscode-script",
    "simonhe.export-what",
    "simonhe.fast-switch-theme",
    "simonhe.symbol-mapping-conversion",
    "simonhe.vscode-css-selector",
    "simonhe.vscode-care",
    "simonhe.unot",
    "simonhe.vscode-simon-zip",
    "simonhe.vscode-eslint-disable-keypress",
    "simonhe.filename-detection",
    "simonhe.lazy-install",
    "simonhe.vscode-generate-preset",
    "simonhe.fast-jump",
    "simonhe.v-component-prompter",
    "simonhe.vscode-highlight-text",
    "simonhe.vscode-toggle-dynamic-prop",
    "simonhe.generate-variable-names",
    "simonhe.fast-create-variable",
    "simonhe.vscode-translate-simon",
    "simonhe.common-intellisense",
    "simonhe.auto-click",
    "simonhe.vscode-tailwind-magic",
    "simonhe.to-tailwindcss"
  ]
